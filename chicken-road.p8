pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function prewarm_cl(cl)
 step=cl.s*30*cl.r
 it=128/step
 for i=0,it do
   ax=i*step
   if cl.s<0 then ax=(136-ax) end
   add(cars,{x=ax, y=cl.y, s=cl.s})
 end
end

function _init()
 print("♥")
 px=64
 py=128-16
 dead=false
 cars={}

 // spawn car lanes
 carlanes={}
 add(carlanes,{y=96,s=0.8,r=1.5,t=t()})
 // prewarm car lanes with cars
 for i=1,#carlanes do
  prewarm_cl(carlanes[i])
 end

 wasbuttons()
end

function _update()
 if btnleft() then
  px-=8
 end

 if btnright() then
  px+=8
 end

 if btnup() then
  py-=8
 end

 if btndown() then
  py+=8
 end

 for i=1,#carlanes do
  cl=carlanes[i]
  if t()-cl.t >= cl.r then
   // spawn car
   x=-8
   if cl.s<0 then x=136 end
   add(cars,{x=x, y=cl.y, s=cl.s})

   cl.t+=cl.r //reset spawn timer
  end
 end

 i=#cars
 while i>0 do
  c=cars[i];
  c.x += c.s

  // player collision
  pb={x=px,y=py,w=8,h=8}
  cb={x=c.x,y=c.y,w=8,h=8}
  if aabb_overlap(pb,cb) then
   dead=true
  end

  // delete out of bounds cars
  if (c.s>0 and c.x>136) or
     (c.s<0 and c.x<-8) then
   deli(cars, i)
  end

  i-=1
 end

 wasbuttons()
end

function _draw()
 cls()
 map()
 spr(1,px,py)

 palt(0,false)
 palt(15,true)
 for i=1,#cars do
  spr(2,cars[i].x,cars[i].y)
 end
 palt(0,true)
 palt(15,false)

 if dead==true then
  cls(rnd(16))
 end
end
