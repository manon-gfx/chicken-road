pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
 print("♥")
 px=64
 py=128-16
 dead=false
 cars={}
 add(cars,{x=0,y=96})

 wasbuttons()
end

function _update()
 if btnleft() then
  px-=8
 end

 if btnright() then
  px+=8
 end

 if btnup() then
  py-=8
 end

 if btndown() then
  py+=8
 end

 for i=1,#cars do
  cars[i].x += 1

  pb={x=px,y=py,w=8,h=8}
  cb={x=cars[i].x,y=cars[i].y,w=8,h=8}
  if aabb_overlap(pb,cb) then
   dead=true
  end
 end

 wasbuttons()
end

function _draw()
 cls()
 map()
 spr(1,px,py)

 palt(0,false)
 palt(15,true)
 for i=1,#cars do
  spr(2,cars[i].x,cars[i].y)
 end
 palt(0,true)
 palt(15,false)

 if dead==true then
  cls(rnd(16))
 end
end
-->8
-- test overlap between bounding
-- boxes
function aabb_overlap(b0, b1)
 if b0.x<b1.x+b1.w and
    b0.x+b0.w>b1.x and
    b0.y<b1.y+b1.h and
    b0.y+b0.h>b1.y then
  return true
 else
  return false
 end
end
-- translate aabb
function trans_aabb(b, x, y)
  return{
   x=b.x+x,
   y=b.y+y,
   w=b.w,
   h=b.h
  }
end
function btnleft()
 return btn(⬅️) and not wasleft
end

function btnright()
 return btn(➡️) and not wasright
end

function btnup()
 return btn(⬆️) and not wasup
end

function btndown()
 return btn(⬇️) and not wasdown
end

function wasbuttons()
 wasleft=btn(⬅️)
 wasright=btn(➡️)
 wasup=btn(⬆️)
 wasdown=btn(⬇️)
end
