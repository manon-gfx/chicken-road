pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
 print("♥")
 px=64
 py=64
end

function _update()
 if btn(⬅️) then
  px-=1
 end

 if btn(➡️) then
  px+=1
 end

 if btn(⬆️) then
  py-=1
 end

 if btn(⬇️) then
  py+=1
 end


end

function _draw()
 cls()
 spr(1,px,py)
end
